{
    "name": "fesor/request-objects",
    "description": "Custom request objects for Symfony made to make life less painful",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergey Protko",
            "email": "fesors@gmail.com"
        }
